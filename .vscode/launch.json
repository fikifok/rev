{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Streamlit: Run app.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/.venv/Scripts/streamlit",
      "args": ["run", "app.py"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
